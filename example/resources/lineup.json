{
  "urls": {
    "https://#env#.otto.de": {
      "paths": [
        "/",
        "multimedia"
      ],
      "max-diff": 2,
      "env-mapping": {
        "live": "www"
      }
    },
    "http://#env#.ottogroup.com" : {
      "paths": [
        "de"
      ],
      "max-diff": 2,
      "env-mapping": {
        "live": "www"
      }
    }
  },
  "browser": "firefox",
  "resolutions": [
    600,
    800
  ]
}